
<!--<body class="hold-transition skin-blue sidebar-mini">-->
    <div class="wrapper">
        <header class="main-header">
            <!-- Logo -->
            <a href="/" class="logo">
                <!-- mini logo for sidebar mini 50x50 pixels -->
                <span class="logo-mini">AS</span>
                <!-- logo for regular state and mobile devices -->
                <span class="logo-lg"><b>Ads</b>System</span>
            </a>
            <!-- Header Navbar: style can be found in header.less -->
            <nav class="navbar navbar-static-top" role="navigation">
                <!-- Sidebar toggle button-->
                <a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button">
                    <span class="sr-only">Toggle navigation</span>
                </a>
                <div class="navbar-custom-menu">
                    <ul class="nav navbar-nav">
                        
                    </ul>
                </div>
            </nav>
        </header>    
        <aside class="main-sidebar">
            <section class="sidebar">
                <ul class="sidebar-menu" id="menu">
                    <li><a href="/users"><i class="fa fa-users"></i> <span>Пользователи</span></a></li>
                    <li><a href="/zones"><i class="fa fa-mobile"></i> <span>Зоны</span></a></li>
                </ul>
            </section>
        </aside>
        <div class="content-wrapper">
            {{#if title}}
                <section class="content-header">
                    <h1>{{title}}</h1>
                </section>
            {{/if}}
            <div class="col-xs-12">
                <div class="box">
                    <div class="box-body">
                        {{#each errors}}
                            <div class="alert alert-danger" role="alert">{{this}}</div>
                        {{/each}}
                        {{{content}}}
                    </div>
                </div>
            </div>
            
        </div><!-- /.content-wrapper -->
        <footer class="main-footer">
            <strong>&copy; 2017 <a href="http://i.atnartur.ru" target="_blank">Артур Атнагулов</a>.</strong>
            Дизайн <a href="http://almsaeedstudio.com" target="_blank">Almsaeed Studio</a>.
        </footer>
    </div><!-- ./wrapper -->